# React App Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_SOCKET_URL=http://localhost:5000

# Google OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id

# Google Maps Configuration
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Cloudinary Configuration (for direct uploads)
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
REACT_APP_CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# App Configuration
REACT_APP_NAME=Agricultural Land Marketplace
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Platform for buying, selling and leasing agricultural land

# Feature Flags
REACT_APP_ENABLE_GOOGLE_OAUTH=true
REACT_APP_ENABLE_GOOGLE_MAPS=true
REACT_APP_ENABLE_LEAFLET_MAPS=false
REACT_APP_ENABLE_REAL_TIME_CHAT=false
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=true

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-tracking-id

# Development Settings
REACT_APP_DEBUG_MODE=true
GENERATE_SOURCEMAP=true

# Build Configuration
REACT_APP_BUILD_DATE=build-date-placeholder
REACT_APP_GIT_COMMIT=git-commit-placeholder

# PWA Configuration
REACT_APP_PWA_NAME=FarmLand
REACT_APP_PWA_SHORT_NAME=FarmLand
REACT_APP_PWA_THEME_COLOR=#22c55e
REACT_APP_PWA_BACKGROUND_COLOR=#ffffff

# Contact Information
REACT_APP_SUPPORT_EMAIL=support@farmland.com
REACT_APP_SUPPORT_PHONE=+1-800-FARMLAND

# Social Media Links
REACT_APP_FACEBOOK_URL=https://facebook.com/farmlandmarketplace
REACT_APP_TWITTER_URL=https://twitter.com/farmlandmarket
REACT_APP_INSTAGRAM_URL=https://instagram.com/farmlandmarketplace